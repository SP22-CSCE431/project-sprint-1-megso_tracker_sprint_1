<p id="notice"><%= notice %></p>

<h1>Attendee Lists</h1>

<table>
  <thead>
    <tr>
      <th>Compound Key</th>
      <th>Attendee List</th>
      <th>UID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendee_lists.each do |attendee_list| %>
      <tr>
        <td><%= attendee_list.compound_key %></td>
        <td><%= attendee_list.attendee_list_id %></td>
        <td><%= attendee_list.UID %></td>
        <td><%= link_to 'Show', attendee_list %></td>
        <td><%= link_to 'Edit', edit_attendee_list_path(attendee_list) %></td>
        <td><%= link_to 'Delete', attendee_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Register for Event', new_attendee_list_path %>
